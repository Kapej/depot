<h1>Listing usets</h1>
<% if notice %>
    <p id="notice"><%= notice%></p>
<% end %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @usets.each do |uset| %>
      <tr>
        <td><%= uset.name %></td>
        <td><%= link_to 'Show', uset %></td>
        <td><%= link_to 'Edit', edit_uset_path(uset) %></td>
        <td><%= link_to 'Destroy', uset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uset', new_uset_path %>
